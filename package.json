{
  "name": "global-salary-explorer",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:browser": "vitest --config vitest.browser.config.ts --run --reporter=dot",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "data:convert": "node scripts/convert-salaries.mjs",
    "storybook": "cross-env STORYBOOK=1 storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^2.0.1",
    "d3": "^7.9.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.22",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.5.1",
    "world-countries": "^5.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@storybook/vue3-vite": "^9.1.13",
    "@tsconfig/node22": "^22.0.2",
    "@types/d3": "^7.4.3",
    "@types/geojson": "^7946.0.16",
    "@types/google.maps": "^3.58.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.18.6",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/browser": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.13",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "cross-env": "^10.1.0",
    "csvtojson": "^2.0.10",
    "eslint": "^9.33.0",
    "eslint-plugin-playwright": "^2.2.2",
    "eslint-plugin-vue": "~10.4.0",
    "i18n-iso-countries": "^7.14.0",
    "jiti": "^2.5.1",
    "jsdom": "^27.0.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "sass": "^1.93.2",
    "typescript": "~5.9.0",
    "vite": "^7.1.7",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vitest": "^3.2.4",
    "vue-component-meta": "^3.1.1",
    "vue-tsc": "^3.1.0"
  }
